{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.25",
        "algolia/algoliasearch-laravel": "1.7.1",
        "barryvdh/laravel-debugbar": "v3.0.0",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "~3.3",
        "hieu-le/active": "^3.5",
        "jhaoda/socialite-mailru": "^3.0",
        "jhaoda/socialite-odnoklassniki": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "~1.0",
        "socialiteproviders/twitter": "^3.0",
        "socialiteproviders/vkontakte": "^3.0",
        "spatie/laravel-fractal": "^5.3",
        "stripe/stripe-php": "^6.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "barryvdh/laravel-ide-helper": "^2.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
